<%= form_tag auth_path, id:"login",class: "navbar-form navbar-right login",role:"form" do %>

 <%= submit_tag "Login", class: "chunky" %>
 <%= password_field_tag :password, params[:password],class: 'form-control login_input',placeholder:"Password", tabindex:'2' %>
 <%= text_field_tag :email, params[:email],class: 'form-control login_input',placeholder:"Email",tabindex:'1' %>
<% end %>